{namespace templates_<%=template_name%>}
/**
 * Container Template
 * @param item Item in context
 */
{template .<%=template_name%>}
    <div class="bp-container <%=bundle_prefix%>-<%=template_name_dashed%>" data-pid="{$item.name}">
        <div class="bp-area <%=bundle_prefix%>-<%=template_name_dashed%>--area">
            {foreach $child in $item.children}
            	{include($child) |noAutoescape}
            {/foreach}
        </div>
    </div>
{/template}